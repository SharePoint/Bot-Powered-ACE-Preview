{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
    "manifestVersion": "1.9",
    "version": "1.0.0",
    "id": "<<Microsoft-App-ID>>",
    "packageName": "FooBar",
    "developer": {
        "name": "Microsoft",
        "websiteUrl": "https://mwww.microsoft.com",
